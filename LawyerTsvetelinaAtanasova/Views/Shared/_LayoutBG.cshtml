                                    <img src="~/Images/bg.gif" alt="" />
                                </a>
                            </li>
                            <li class="" dir="ltr">
                                <a href="@Url.Action("StartSeite", "Home")">
                                    <img src="~/Images/de.gif" alt="German (DE)" title="German (DE)" />
                                </a>
                            </li>
                            <li class="lang-active" dir="ltr">
                                <a href="@Url.Action("Index", "Home")">
                                    <img src="~/Images/en.gif" alt="English (UK)" title="English (UK)" />
                                </a>
                            </li>
                        </ul>

                    </div>
